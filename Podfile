# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
use_frameworks!

def common_pods
  pod 'Swinject'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Reusable'
  pod 'SwiftLint'
  pod 'RxSwiftUtilities'
  pod 'RxSwiftExt'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'RxFirebase/Firestore'
  pod 'RxFirebase/Auth'
  
end

target 'PokerPlanning' do
	inhibit_all_warnings!
  	common_pods
end
